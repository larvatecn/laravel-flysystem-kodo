{
  "name": "larva/laravel-flysystem-kodo",
  "description": "This is a Flysystem adapter for the Qiniu Kodo.",
  "keywords": [ "flysystem", "qiniu", "kodo", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0",
    "league/flysystem": "^3.0",
    "larva/flysystem-kodo": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Flysystem\\Qiniu\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Flysystem\\Qiniu\\ObjectStorageServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
